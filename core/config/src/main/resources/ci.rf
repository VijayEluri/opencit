        <constraint toNodeId="39" name="testsFinished" priority="1" >v : Value( key == "testId")
TestEndEvent( testId == (String) v.getValue() )</constraint>
      </constraints>
    </state>
    <split id="39" name="Split" x="90" y="608" width="80" height="40" type="2" >

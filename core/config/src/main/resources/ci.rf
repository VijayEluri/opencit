    <state id="100" name="WaitForDeployToFinish" x="25" y="801" width="181" height="40" >
      <constraints>
        <constraint toNodeId="104" name="deployFinished" priority="1" >v : Value( key == "testId")
TestEndEvent( testId == v.getValue() )</constraint>
        <constraint toNodeId="20" name="constraint" priority="1" >v : Value( key == "deployId")
DeployEndEvent( deployId == v.getValue() )</constraint>
      </constraints>
    </state>
    <ruleSet id="101" name="SendReport" x="439" y="883" width="80" height="48" ruleFlowGroup="finishWorkflow" />
    <state id="38" name="WaitForTestsToFinish" x="42" y="514" width="146" height="40" >
